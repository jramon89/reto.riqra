    "style-loader": "^0.13.2",
    "uglifyjs-webpack-plugin": "^1.2.4",
    "url": "^0.11.0",
    "url-loader": "^0.5.8",
    "webpack": "^4.3.0",
    "webpack-clean-plugin": "^0.2.3",
    "webpack-cli": "^3.3.5",
