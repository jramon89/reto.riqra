{
  "name": "imessage",
  "version": "1.0.0",
  "description": "imessage a instant message app",
  "main": "index.js",
  "scripts": {
    "server": "better-npm-run server-start",
    "dev": "better-npm-run development",
    "build": "better-npm-run production"
  },
  "betterScripts": {
    "development": {
      "command": "node ./webpack/webpack.dev.js",
      "env": {
        "HOST": "localhost",
        "PORT": 8081,
        "MODE": "development"
      }
    },
    "production": {
      "command": "webpack --config ./webpack/webpack.prod.js",
      "env": {
        "HOST": "",
        "PORT": "",
        "MODE": "production"
      }
    },
    "server-start": {
      "command": "node ./server/server.js",
      "env": {
        "HOST": "localhost",
        "PORT": 4000
      }
    }
  },
  "keywords": [
    "react",
    "es6",
    "node"
  ],
  "author": "Jose Ramon Roblero",
  "license": "ISC",
  "dependencies": {
    "apollo-boost": "^0.4.3",
    "es6-promise": "^4.2.8",
    "express": "^4.17.3",
    "express-graphql": "^0.8.0",
    "graphql": "^14.4.0",
    "isomorphic-fetch": "^2.2.1",
    "mysql2": "^1.6.5",
    "node-fetch": "^2.6.0",
    "nodemon": "^1.19.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-dom": "^16.8.6",
    "react-redux": "^5.1.1",
    "react-router": "^4.3.1",
    "react-router-config": "^4.4.0-beta.8",
    "react-router-dom": "^4.3.1",
    "redux": "^3.7.2",
    "redux-thunk": "^2.3.0",
    "sequelize": "^5.8.12"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "better-npm-run": "0.0.15",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "css-loader": "^0.26.4",
